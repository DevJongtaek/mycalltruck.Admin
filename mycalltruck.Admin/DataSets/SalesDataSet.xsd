          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SalesReportTableAdapter" GeneratorDataComponentClassName="SalesReportTableAdapter" Name="SalesReport" UserDataComponentName="SalesReportTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="TruckConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  Clients.BizNo, Clients.Name, Clients.CEO, Clients.PhoneNo, Clients.Uptae, Clients.Upjong, Clients.AddressState + ' ' + Clients.AddressCity + ' ' + Clients.AddressDetail AS ClientsAddr, 
               Customers.BizNo AS CBizNo, Customers.SangHo, Customers.Ceo AS CCeo, Customers.PhoneNo AS CPhoneNo, Customers.Uptae AS CUptae, Customers.Upjong AS CUpjong, 
               Customers.AddressState + ' ' + Customers.AddressCity + Customers.AddressDetail AS CustomerAddr, CONVERT(varchar(10), Orders.CreateTime, 120) AS AcceptDate, Orders.StartName, 
               Orders.StopName, Orders.Item
			 , CASE WHEN SalesManage.UseTax = 1 THEN ISNULL(Orders.SalesPrice, 0)
					 WHEN SalesManage.Vat != 0 THEN ISNULL(CEILING(Orders.SalesPrice / 1.1), 0) 
				     ELSE ISNULL(Orders.SalesPrice, 0) END AS Price
			   , CASE WHEN SalesManage.UseTax = 1 THEN ISNULL(FLOOR(Orders.SalesPrice * 0.1), 0) 
					  WHEN SalesManage.Vat != 0 THEN ISNULL(Orders.SalesPrice, 0) - ISNULL(CEILING(Orders.SalesPrice / 1.1), 0) 
					  ELSE 0 END AS VAT, 
               CASE WHEN SalesManage.UseTax = 1 THEN ISNULL(Orders.SalesPrice, 0) + ISNULL(FLOOR(Orders.SalesPrice * 0.1), 0) 
               WHEN SalesManage.Vat != 0 THEN ISNULL(CEILING(Orders.SalesPrice / 1.1), 0) + ISNULL(Orders.SalesPrice, 0) - ISNULL(CEILING(Orders.SalesPrice / 1.1), 0) 
               ELSE ISNULL(Orders.SalesPrice, 0) + 0 END AS Amont
				,SalesManage.Amount as Amont
               , Orders.SalesManageId, CONVERT(varchar(10), SalesManage.BeginDate, 120) 
               + '~' + CONVERT(varchar(10), SalesManage.EndDate, 120) AS Gigan, ISNULL(SalesManage.CMSBankName, Clients.CMSBankName) AS CMSBankName, ISNULL(SalesManage.CMSAccountNo, 
